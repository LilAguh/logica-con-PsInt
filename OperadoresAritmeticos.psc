Algoritmo OperadoresAritmeticos
	// Operadores aritmeticos
	
	// Suma +
	a = 3 + 4
	Imprimir 'suma: ', a
	
	// Resta -
	b = 6 - 2
	Imprimir 'Resta: ', b
	
	// Multiplicacion *
	c = a * 2
	Imprimir 'Multiplicacion: ', c
	
	// Division /
	d = a / b
	Imprimir 'Multiplicacion: ', d
	
	//Potencia ^
	e = a ^ 2
	Imprimir 'Potencia: ', e
	
	// Resto %
	f = 11 % 4
	Imprimir 'Resto: ', f
FinAlgoritmo
